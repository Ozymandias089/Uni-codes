packadd! dracula
syntax enable
colorscheme dracula
hi Normal guibg=NONE ctermbg=NONE

set tabstop=4
set softtabstop=4
set expandtab
set number
set autoindent
set smartindent
set showcmd
set cursorline
filetype indent on
set wildmenu
set lazyredraw
set showmatch
set incsearch
set hlsearch
set foldenable
set foldlevelstart=10
nnoremap <space> za
set foldmethod=indent
set autoindent
set laststatus=2
